<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="RoslynTools.RepoToolset">
  <PropertyGroup>
    <AssemblyName>Microsoft.DotNet.Build.Tasks.Feed</AssemblyName>
    <TargetFramework>net45</TargetFramework>
    <NuGetRuntimeIdentifier>win7-x64</NuGetRuntimeIdentifier>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <PackageTargetFallback>dotnet;net46</PackageTargetFallback>

    <!-- We hit package version conflicts due to assembly version mismatches, we are expecting those and ignore them for this project. -->
    <NoWarn>$(NoWarn);NU1605</NoWarn>
    <ResolveAssemblyReferencesSilent>True</ResolveAssemblyReferencesSilent>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)..\Compile.props" />

  <ItemGroup>
    <!-- RepoToolset adds packagereferences to Microsoft.Net.Compilers and Xlifftasks that we don't need -->
    <PackageReference Remove="@(PackageReference)" />
  
    <PackageReference Include="Microsoft.DotNet.VersionTools" Version="2.1.0-preview2-02515-0"/>
    <PackageReference Include="NuGet.Versioning" Version="4.3.0" />
    <PackageReference Include="NuGet.Packaging" Version="4.3.0" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
    <PackageReference Include="SleetLib" Version="2.2.24" />
    <PackageReference Include="System.Reflection.Metadata" Version="1.4.1" />
    <PackageReference Include="System.Runtime.InteropServices.RuntimeInformation" Version="4.4.0-beta-24813-03" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.Build.Tasks.v4.0" />
    <Reference Include="Microsoft.Build.Framework" />
    <Reference Include="Microsoft.Build.Utilities.v4.0" />
    <Reference Include="System" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Collections" />
    <Reference Include="System.Core" />
    <Reference Include="System.IO" />
    <Reference Include="System.IO.Compression" />
    <Reference Include="System.IO.Compression.FileSystem" />
    <Reference Include="System.Runtime" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>
</Project>